ifndef config
  config=debug
endif
export config

PROJECTS := fbx-conv

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

fbx-conv: 
	@echo "==== Building fbx-conv ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f fbx-conv.make

clean:
	@${MAKE} --no-print-directory -C . -f fbx-conv.make clean

help:
